@model DXPANACEASOFT.Models.DTOModel.MasteredDTO

@Html.DevExpress().ComboBoxFor(model => model.id_warehouseBoxes, settings =>
{
	settings.Name = "ComboBoxWarehouseBoxes";
	settings.Width = Unit.Percentage(100);
	//settings.Properties.NullText = "Todos...";
	settings.CallbackRouteValues = new
	{
		Controller = "Mastered",
		Action = "ComboBoxWarehouseBoxes",
	};
	settings.Properties.ClientSideEvents.SelectedIndexChanged = "ComboBoxWarehouseBoxes_SelectedIndexChanged";
	settings.Properties.ClientSideEvents.BeginCallback = "ComboBoxWarehouseBoxes_BeginCallback";
	settings.Properties.ClientSideEvents.EndCallback = "ComboBoxWarehouseBoxes_EndCallback";
	settings.Properties.CallbackPageSize = 15;
	settings.Properties.TextField = "Text";
	settings.Properties.ValueField = "Value";
	settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.OnHover;
	//settings.Properties.ValidationSettings.RequiredField.IsRequired = true;
	settings.ReadOnly = !((bool)ViewBag.enabled);
}).BindList(ViewData["WarehouseBoxes"]).GetHtml()
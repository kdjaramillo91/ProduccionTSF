@model DXPANACEASOFT.Models.DTOModel.MasteredDTO

@Html.DevExpress().TokenBoxFor(model => model.ids_warehouseLocationBoxes, settings =>
{
    settings.Name = "TokenBoxWarehouseLocationsBoxes";
    settings.Width = Unit.Percentage(100);
    //settings.Properties.NullText = "Todos...";
    settings.CallbackRouteValues = new
    {
        Controller = "Mastered",
        Action = "TokenBoxWarehouseLocationsBoxes",
    };
    settings.Properties.ClientSideEvents.BeginCallback = "TokenBoxWarehouseLocationsBoxes_BeginCallback";
    settings.Properties.ClientSideEvents.EndCallback = "TokenBoxWarehouseLocationsBoxes_EndCallback";
    settings.Properties.ClientSideEvents.ValueChanged = "TokenBoxWarehouseLocationsBoxes_ValueChanged";
    settings.Properties.CallbackPageSize = 15;
    settings.Properties.TextField = "Text";
    settings.Properties.ValueField = "Value";
    settings.Properties.AllowCustomTokens = false;
    settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.OnHover;
    //settings.Properties.ValidationSettings.RequiredField.IsRequired = true;

@model DXPANACEASOFT.Models.DTOModel.MasteredDTO

@Html.DevExpress().ComboBoxFor(model => model.id_masteredWarehouse, settings =>
{
	settings.Name = "ComboBoxMasteredWarehouse";
	settings.Width = Unit.Percentage(100);
	//settings.Properties.NullText = "Todos...";
	settings.CallbackRouteValues = new
	{
		Controller = "Mastered",
		Action = "ComboBoxMasteredWarehouse",
	};
	settings.Properties.ClientSideEvents.SelectedIndexChanged = "ComboBoxMasteredWarehouse_SelectedIndexChanged";
	settings.Properties.ClientSideEvents.BeginCallback = "ComboBoxMasteredWarehouse_BeginCallback";
	settings.Properties.ClientSideEvents.EndCallback = "ComboBoxMasteredWarehouse_EndCallback";
	settings.Properties.CallbackPageSize = 15;
	settings.Properties.TextField = "Text";
	settings.Properties.ValueField = "Value";
	settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.OnHover;
	//settings.Properties.ValidationSettings.RequiredField.IsRequired = true;
	settings.ReadOnly = !((bool)ViewBag.enabled);
}).BindList(ViewData["MasteredWarehouse"]).GetHtml()
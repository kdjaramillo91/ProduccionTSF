@model DXPANACEASOFT.Models.DTOModel.MasteredDTO

@Html.DevExpress().ComboBoxFor(model => model.id_warehouseLocationBoxes, settings =>
{
    settings.Name = "ComboBoxWarehouseLocationBoxes";
    settings.Width = Unit.Percentage(100);
    settings.Properties.NullText = "Todos...";
    settings.CallbackRouteValues = new
    {
        Controller = "Mastered",
        Action = "ComboBoxWarehouseLocationBoxes",
    };
    settings.Properties.ClientSideEvents.BeginCallback = "ComboBoxWarehouseLocationBoxes_BeginCallback";
    //settings.Properties.ClientSideEvents.EndCallback = "ComboBoxWarehouseLocationBoxes_EndCallback";
    //settings.Properties.ClientSideEvents.ValueChanged = "ComboBoxWarehouseLocationBoxes_ValueChanged";
    settings.Properties.CallbackPageSize = 15;
    settings.Properties.TextField = "Text";
    settings.Properties.ValueField = "Value";
    settings.Properties.ClearButton.DisplayMode = ClearButtonDisplayMode.OnHover;
    settings.ReadOnly = !((bool)ViewBag.enabled);
}).BindList(ViewData["WarehouseLocationBoxes"]).GetHtml()


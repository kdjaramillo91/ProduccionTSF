@model DXPANACEASOFT.Models.IntegrationState

@Html.DevExpress().Button(settings =>
{
    settings.Name = "btnNewDetail";
    //settings.Width = 90;
    settings.Text = "";
    //settings.RenderMode = ButtonRenderMode.Link;
    settings.Images.Image.IconID = IconID.ActionsAdditem16x16office2013;
    settings.ClientEnabled = true;//(Model.isReadOnly) ? false : true;
    settings.UseSubmitBehavior = false;
    settings.EnableClientSideAPI = true;
    //settings.ClientSideEvents.Click = "AddNewDocument";
    settings.ClientSideEvents.Click = "OIntegrationProcess.AddNewDocument";
    
    settings.ToolTip = "Agregar Documento";


@model DXPANACEASOFT.Models.DTOModel.AutomaticTransferDTO

@{ 
	bool canShowButton = ViewBag.canShowButton != null ? (bool)ViewBag.canShowButton : false;

}

<div>
	@{
		ViewData["id_warehouse_exit"] = Model.id_WarehouseExit;
		ViewData["id_warehouse_location_exit"] = Model.id_WarehouseLocationExit;
	}
		
	<div class="MyContainer" id="divGridViewDetails">
		<div class="row">
			@if (canShowButton)
			{
				@Html.Partial("PanelButtons/_DetailsActionsButtons")
			}


		</div>
		<div class="row">
			<div id="search_option_inventoryMove" class="widget-box">
				<div class="widget-header">
					<h6 class="widget-title">Filtros de B&uacute;squeda</h6>
					<span class="widget-toolbar">
						<a href="#" id="btnCollapseFilterDetails" data-action="collapse">
							<i id="filterFormIcon" class="ace-icon fa fa-chevron-up"></i>
						</a>
					</span>
				</div>
				<div id="filterForm" class="widget-body">
					<div class="widget-main">
						@Html.Partial("ComponentsDetail/_FilterDetails", Model)
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			@Html.Partial("_GridViewDetailItem", Model.lsDetail)
		</div>
	</div>
</div>
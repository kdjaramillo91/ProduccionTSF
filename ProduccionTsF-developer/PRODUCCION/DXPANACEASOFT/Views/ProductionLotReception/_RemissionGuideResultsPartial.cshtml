
@model List<DXPANACEASOFT.Models.RemissionGuide>

<div id="GridMessageErrorPurchaseOrdersAndRemissionGuides" style="display: none;">
    @Html.DevExpress().Label(settings =>
       {
           settings.Name = "gridMessageErrorPurchaseOrdersAndRemissionGuides";
           settings.Properties.ClientInstanceName = "gridMessageErrorPurchaseOrdersAndRemissionGuides";
           settings.Text = "";
       }).GetHtml()
</div>


@Html.DevExpress().Button(settings =>
{
    settings.Name = "btnGenerateLot";
    settings.Text = "Generar Lote";
    settings.Images.Image.IconID = IconID.ActionsAdditem16x16office2013;
    settings.ClientEnabled = true;
    settings.UseSubmitBehavior = false;
    settings.EnableClientSideAPI = true;
    settings.ClientSideEvents.Click = "GenerateLotGuiaRemision";
}).GetHtml()

<hr style="margin-top: 2px; margin-bottom: 2px;" />

@{
    Html.RenderPartial("_RemissionGuidePartial", Model);
}

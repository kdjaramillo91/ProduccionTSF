@model DXPANACEASOFT.Models.DTOModel.InventoryMovePlantTransferDTO

@Html.Hidden("id_inventoryMovePlantTransfer", Model.id)
@Html.Hidden("enabled", ((bool)ViewBag.enabled))
@Html.Hidden("timeInitTurn", Model.timeInitTurn)
@Html.Hidden("timeEndTurn", Model.timeEndTurn)

@Html.DevExpress().PageControl(settings =>
{
	settings.Name = "pcFeatures";
	settings.Width = Unit.Percentage(100);
	settings.Height = Unit.Pixel(510);
	settings.ActiveTabIndex = 1;
	settings.TabPages.Add("Documento").SetContent(() =>
	{
		ViewContext.Writer.Write(
			"<div id='divTabDocumentEdit'>" +
			@Html.Partial("_TabDocumentEdit", Model)
			+ "</div>");
	});
	settings.TabPages.Add("Carro por Carro").SetContent(() =>
	{
		ViewContext.Writer.Write(
			"<div id='divTabCartOnCartEdit'>" +
			@Html.Partial("_TabCartOnCartEdit", Model)
			+ "</div>");
	});
	settings.TabPages.Add("Movimiento").SetContent(() =>
	{
		ViewContext.Writer.Write(
			"<div id='divTabMoveEdit'>" +
			@Html.Partial("_TabMoveEdit", Model)
			+ "</div>");
	});
	settings.TabPages.Add("Detalles").SetContent(() =>
	{
		ViewContext.Writer.Write(
			"<div id='divTabDetailsEdit'>" +
			@Html.Partial("_TabDetailsEdit", Model)
			+ "</div>");
	});
}).GetHtml()


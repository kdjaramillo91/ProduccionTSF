
@model int

<table style="width: 100%; border: none;">
    <tr>
        <td>
            @Html.DevExpress().Button(settings =>
            {

                settings.Name = "btnPrint_" + Model.ToString();
                settings.ToolTip = "Imprimir";
                settings.RenderMode = ButtonRenderMode.Link;
                settings.Text = "";
                settings.Images.Image.IconID = IconID.ActionsPrint16x16devav;
                settings.ClientEnabled = true;
                settings.UseSubmitBehavior = false;
                settings.EnableClientSideAPI = true;
                settings.ClientSideEvents.Click = "PrintDetailGD";
            }).GetHtml()
        </td>
    </tr>
</table>

    

    

    

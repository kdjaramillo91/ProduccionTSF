<div id="GridMessageErrorRemissionGuideCVE" style="display: none;">
    @Html.DevExpress().Label(settings =>
       {
           settings.Name = "gridMessageErrorRemissionGuideCVE";
           settings.Properties.ClientInstanceName = "gridMessageErrorRemissionGuideCVE";
           settings.Text = "";
       }).GetHtml()
</div>
@Html.DevExpress().Button(settings =>
{
    settings.Name = "btnAssignDateTimeRemissionGuideControlEntrance";

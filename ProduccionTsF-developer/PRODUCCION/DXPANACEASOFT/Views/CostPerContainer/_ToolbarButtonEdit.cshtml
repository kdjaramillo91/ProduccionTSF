<div style="width: 100%;">
	@Html.DevExpress().Button(
		button =>
		{
			button.Name = "btnUpdateToolBar";
			button.Text = "";
			button.ToolTip = "Guardar documento";
			button.Images.Image.IconID = IconID.ActionsSave16x16devav;
			button.UseSubmitBehavior = false;
			button.EnableClientSideAPI = true;
			button.ClientSideEvents.Click = "OnUpdateButtonClick";
			button.ClientVisible = (bool)this.ViewBag.PuedeEditarDocumento;
		}).GetHtml()

	@Html.DevExpress().Button(
		button =>
		{
			button.Name = "btnRefreshToolBar";
			button.Text = "";
			button.ToolTip = "Refrescar";
			button.Images.Image.IconID = IconID.ActionsRefresh16x16office2013;
			button.UseSubmitBehavior = false;
			button.EnableClientSideAPI = true;
			button.ClientSideEvents.Click = "OnRefreshButtonClick";
			button.ClientEnabled = (bool)this.ViewBag.DocumentoExistente;
		}).GetHtml()
	
<hr style="margin-top: 2px; margin-bottom: 2px;" />



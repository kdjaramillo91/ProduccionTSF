
@model List<DXPANACEASOFT.Models.RemissionGuide>

<div id="GridMessageErrorRemisionGuide" style="display: none;">
    @Html.DevExpress().Label(settings =>
       {
           settings.Name = "gridMessageErrorRemisionGuide";
           settings.Properties.ClientInstanceName = "gridMessageErrorRemisionGuide";
           settings.Text = "";
       }).GetHtml()
</div>

@Html.DevExpress().Button(settings =>
{
    settings.Name = "btnGenerateReceptionDispatchMaterials";
    settings.Text = "Asignar Recepción";
    settings.Images.Image.IconID = IconID.ActionsAdditem16x16office2013;
    settings.ClientEnabled = true;
    settings.UseSubmitBehavior = false;
    settings.EnableClientSideAPI = true;
    settings.ClientSideEvents.Click = "GenerateReceptionDispatchMaterials";
}).GetHtml()

<hr style="margin-top: 2px; margin-bottom: 2px;" />

@{
    Html.RenderPartial("_RemissionGuidesPartial", Model);
}


@model List<DXPANACEASOFT.Models.PurchaseOrderDetail>

<div id="GridMessageErrorPurchaseOrder" style="display: none;">
    @Html.DevExpress().Label(settings =>
       {
           settings.Name = "gridMessageErrorPurchaseOrder";
           settings.Properties.ClientInstanceName = "gridMessageErrorPurchaseOrder";
           settings.Text = "";
       }).GetHtml()
</div>

@Html.DevExpress().Button(settings =>
{
    settings.Name = "btnGeneratePurchaseDetailForReassignment";
    settings.Text = "Buscar Guía de Remisión";
    settings.Images.Image.IconID = IconID.ActionsAdditem16x16office2013;
    settings.ClientEnabled = true;
    settings.UseSubmitBehavior = false;
    settings.EnableClientSideAPI = true;
    settings.ClientSideEvents.Click = "GeneratePurchaseDetailForReassignment";
}).GetHtml()


<hr style="margin-top: 2px; margin-bottom: 2px;" />

@{
    Html.RenderPartial("_PurchaseOrderDetailsForReassignmentPartial", Model);
}

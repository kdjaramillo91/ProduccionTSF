@Html.DevExpress().BinaryImage(biSettings =>
{
    biSettings.Width = 160;
    biSettings.Height = 160;
    biSettings.Properties.ImageSizeMode = ImageSizeMode.FitProportional;
    biSettings.Name = "image" + ViewData["id_item"];
    biSettings.Properties.EnableServerResize = true;
}).Bind(Model).GetHtml()
